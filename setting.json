		"clsx",
		"craco",
		"Defi",
		"Presale",
		"reduxjs",
		"solarbk",
		"tailwindcss"
	],
	"editor.fontFamily": "Fira Code, JetBrains Mono, Dank Mono, Cascadia Code",
	"[html]": {
		"editor.defaultFormatter": "vscode.html-language-features"
	},
	"security.workspace.trust.untrustedFiles": "open",
	"editor.codeLensFontFamily": "Dank Mono, Cascadia Code, JetBrains Mono, Fira Code",
	"files.associations": {
		".html": "html",
		".js": "javascript",
		"*.jsx": "javascriptreact"
	},
	"bracketPairColorizer.depreciation-notice": false,
	"files.exclude": {
		"out": false
	},
	"search.exclude": {
		"out": true
	},
	"typescript.tsc.autoDetect": "off",
	"tailwindCSS.classAttributes": [
		"class",
		"className"
	],
	"tailwindCSS.emmetCompletions": true,
	"files.saveConflictResolution": "overwriteFileOnDisk",
	"files.defaultLanguage": "typescript",
	"workbench.editor.wrapTabs": true,
	"workbench.colorTheme": "Dracula",
	"background.backgroundImageSize": "Contain",
	"background.windowBackgrounds": [
		"c:/Users/vinhn/Pictures/image/vscodeBg.png"
	],
	"sync.gist": "7b074405bb0f01900dd686136f5e3e8d",
	"[svg]": {
